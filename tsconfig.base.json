{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2019",
    "module": "ESNext",
    "lib": ["ES2019", "DOM"],
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "baseUrl": "."
  }
}
